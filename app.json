{
  "expo": {
    "name": "Frozen Store",
    "slug": "frozen-store",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.2.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/homepage.jpeg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.frozen.store"
    },
    "description": "Frozen Store E-Commerce",
    "android": {
      "package": "com.frozen.store"
    }
  }
}
